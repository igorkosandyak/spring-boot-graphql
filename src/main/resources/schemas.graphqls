schema {
    query: Query
}

type Query {
    users: [User]
    user(id: String): User
}

type User {
    id: String
    name: String
    age: Int
    nationality: String
    createdAt: String
    friends: [User]
    articles: [Article]
}

type Article {
   id: String
   title: String
   minutesRead: Int
}